#include <stdio.h>
#include <stdlib.h>

char string[40];

int main(void) {
    setvbuf(stdout, NULL, _IONBF, 0);
    setvbuf(stderr, NULL, _IONBF, 0);
	puts("You treveler I great!"); /* prints You treveler I great! */
	fscanf("%s",&string);
	puts(string);
	return EXIT_SUCCESS;
}
